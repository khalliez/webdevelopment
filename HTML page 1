<!-- All different input type https://developer.mozilla.org/en-US/docs/Web/HTML/Element/input -->
<!-- CHECK BOX -->
	<form>
	   <div>
		   <label for = "agreed">  I agree to the terms and conditions: </label>
		   <input id = "agreed" name = "agreed" type="checkbox" required>
		   <!-- Button -->
			<button>Go!</button>

	   </div>
	</form>	

    <!--  default it checked -->
	<div><input name = "agreed" type="checkbox" required checked></div>

<!-- COLOR BOX -->
	<div><input type="color"></div>

<!-- radio button -->
	<h5>Pick cat or dog</h5>
	
		<label  for = "dogs">  Dogs:  </label> 
		<label for = "cats">  Cats:  </label> 
                                 <!-- value is the value that shows in url -->
		<input name = "pet choice" id ="dogs" type="radio" value= "DOG"> 
		<input name = "pet choice" id ="cats" type="radio" value= "CAT">

	<!-- Drop Down Menu -->
	<label>	
	  pick number
		<select name = "pick number" >
			<option value = "">Select...</option>
			<option >1</option>
			<option>2</option>
			<option>3</option>
			<option value = "Not Dog or Cat">Other</option> 
			  <!-- use value for what text to sho win url when option is chosen  HTML/firstpage?pet+choice=CAT&How+Many=Not+Dog+or+Cat-->
		</select>
	</label>

	<!-- Text Area -->	
	<div><textarea name = "Write about" rows = "5" cols = "10">
		Write something here
	</textarea></div>

    <!-- Button -->
		<button>Go!</button>
	</form>

	


<!-- Form tag. Sign In. Password. -->
  <!-- https://developer.mozilla.org/en-US/docs/Web/HTML/Element/form -->
	<h1>Sign In</h1>
	<form action = "http://www.google.com" method="GET">
		<label> Username:    <!-- Label before the input object -->
			<!--        name = adds the text to the url.  placeholder create gray text in input box -->
			<input name = "username" type="text" placeholder = "username" required>  
		</label>

		<label for = "password">  Password 1:  </label>   <!-- for connects to the id in the input -->
		<input id = "password" name = "password" type="password" placeholder = "password" required pattern = ".{5,10}" required title = "Password must be between 5 and 10 characters">

		<label for = "email">  Email:  </label>
		<input id = "email" name = "email" type="email" placeholder = "email" required> <!-- required requires a @ in the text-->
		<input type = "submit">
			<!-- <button>Login</button> -->
	</form>


